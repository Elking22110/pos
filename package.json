{
  "name": "pos-system-modern-ui",
  "version": "2.0.0",
  "description": "نظام إدارة متجر الأزياء الرجالية - واجهة مستخدم حديثة ومتطورة",
  "keywords": [
    "pos",
    "point-of-sale",
    "retail",
    "management",
    "react",
    "modern-ui",
    "arabic",
    "fashion",
    "store",
    "inventory"
  ],
  "author": {
    "name": "نظام إدارة متجر الأزياء الرجالية",
    "email": "info@mensfashion.com",
    "url": "https://mensfashion.com"
  },
  "license": "MIT",
  "homepage": "https://github.com/your-username/pos-system-modern-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/pos-system-modern-ui.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/pos-system-modern-ui/issues"
  },
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src --ext js,jsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,jsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "clean": "rm -rf dist node_modules/.vite",
    "clean:all": "rm -rf dist node_modules package-lock.json",
    "reinstall": "npm run clean:all && npm install",
    "analyze": "npm run build && npx vite-bundle-analyzer dist",
    "security": "npm audit",
    "security:fix": "npm audit fix",
    "update": "npm update",
    "outdated": "npm outdated",
    "size": "npm run build && du -sh dist",
    "serve": "npx serve dist -s -l 3000"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0",
    "framer-motion": "^10.0.0",
    "lucide-react": "^0.294.0",
    "recharts": "^2.7.0",
    "jwt-decode": "^3.1.2",
    "crypto-js": "^4.1.1",
    "lodash": "^4.17.21",
    "date-fns": "^2.30.0",
    "react-hook-form": "^7.47.0",
    "react-query": "^3.39.3",
    "axios": "^1.6.0",
    "react-hot-toast": "^2.4.1",
    "react-intersection-observer": "^9.5.2",
    "react-virtualized": "^9.22.5"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/node": "^20.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.0",
    "postcss": "^8.4.0",
    "prettier": "^3.0.0",
    "tailwindcss": "^3.2.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vitest": "^1.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/your-username"
  },
  "contributors": [
    {
      "name": "المطور الرئيسي",
      "email": "developer@mensfashion.com"
    }
  ],
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE",
    "CHANGELOG.md",
    "INSTALLATION.md"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "config": {
    "port": 5173,
    "host": "localhost"
  }
}